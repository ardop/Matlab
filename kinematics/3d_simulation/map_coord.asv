function [xx, yy, zz] = map_coord(c)
    
    x = c(1);
    y = c(2)'
    
    x_offset = 20;
    y_offset = 0;
    z_offset = 40;
    
    %Now we reorient
    xx = z;
    yy = x;
    zz = y;
    
    %We offset to account for origin shift
    xx = xx + x_offset;
    yy = yy + y_offset;
    zz = zz + z_offset;

    
end