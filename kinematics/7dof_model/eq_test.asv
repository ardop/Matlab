function [a] = eq_test(theta, px, py, pz)

    [theta1, theta2, theta3, theta4, theta5, theta6, theta7] = thetamap(theta);
    
    l1 = 14.2;
    l2 = 1.8;
    l3 = 14.5;
    l5 = 29.5;
    
    a = 0;
    
    disp(cos(theta4));
    
    a = (px*px + py*py + pz*pz + l1*l1 - 2*pz*l1 - l3*l3 + l2*l2 - l5*l5 + 2*l2*px*sin(theta1) - 2*l2*py*cos(theta1))
    
end